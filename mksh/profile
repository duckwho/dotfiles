LC_COLLATE=en_US.UTF-8
LC_CTYPE=en_US.UTF-8
LC_MESSAGES=en_US.UTF-8
LC_MONETARY=en_US.UTF-8
LC_NUMERIC=en_US.UTF-8
LC_TIME=en_US.UTF-8
LC_ALL=en_US.UTF-8
LANG=en_US.UTF-8
LANGUAGE=en_US.UTF-8
LESSCHARSET=utf-8
#TZ= Asia/Seoul


PATH=/bin:/sbin:/usr/bin/:/usr/sbin:/usr/local/bin:/usr/local/sbin
PATH=$PATH:/usr/bin/site_perl:/usr/bin/vendor_perl:/usr/bin/core_perl:$HOME/bin:$HOME/bin/pkgs:$HOME/.cargo/bin
GOPATH=$HOME/.go


XDG_CONFIG_HOME=$HOME/.config
MAIL=/var/mail/duckwho

EDITOR=nvim
VISUAL=$EDITOR
PAGER='less -R'
MANWIDTH=80

DUCK_THEME=simpl
DUCK_COLOR=ish

CC=cc
DEBUGGER=gdb
TRACER=strace

MPD_HOST=duckwho

W3MIMGDISPLAY_PATH=$HOME/bin/misc/w3mfix

QT_QPA_PLATFORMTHEME="qt5ct"

UNZIP="-O cp949"
ZIPINFO="-O cp949"

TASKDDATA=$HOME/var/taskd

export LC_COLLATE LC_CTYPE LC_MESSAGES LC_MONETARY LC_NUMERIC LC_TIME LC_ALL \
       LANG LANGUAGE LESSCHARSET TZ PATH GOPATH XDG_CONFIG_HOME MAIL EDITOR VISUAL \
       PAGER BROWSER MANWIDTH CC DEBUGGER TRACER MPD_HOST QT_QPA_PLATFORMTHEME \
       DUCK_THEME UNZIP ZIPINFO

